@model IEnumerable<Account>
@{ 
    ViewData["Title"] = "Admin";
}

<h1 class="bg-info text-white" >Users</h1>
<table class="table table-bordered">
    <tr >
        <th>Registration Date</th>
        <th>Name</th>
        <th>Email</th>
    </tr>
    @foreach (Account acc in Model)
    {
        <tr>
            <td>@acc.DateOfMembership.ToString("dd.mm.yyyy")</td>
            <td>@acc.UserName</td>
            <td>@acc.Email</td>
        </tr>
    }
</table>
<a class="btn btn-primary" asp-controller="Admin" asp-action="Create">Create user</a>